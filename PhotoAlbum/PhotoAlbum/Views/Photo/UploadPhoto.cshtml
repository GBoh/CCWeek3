@model PhotoAlbum.Domain.Photo

@{
    ViewBag.Title = "UploadPhoto";
}

<h2>UploadPhoto</h2>

@using (Html.BeginForm("UploadPhoto", 
    "Photo", 
    FormMethod.Post, 
    new 
    { enctype = "multipart/form-data" })) {
    <input type="file" name="file" id="file" />
    <input type="submit" value="Upload" />
}

